{
  "name": "api-service",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "test": "npm run lint && cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --coverage --coverageDirectory=coverage",
    "start:prod": "cross-env NODE_ENV=production node ./src/index.js",
    "start:dev": "cross-env NODE_ENV=development node ./src/index.js",
    "start:stag": "cross-env NODE_ENV=staging node ./src/index.js",
    "start:local": "cross-env NODE_ENV=local nodemon ./src/index.js",
    "start": "cross-env NODE_ENV=development node ./src/index.js",
    "start:debug": "cross-env NODE_ENV=development yarn --inspect-brk ./src/index.js",
    "lint": "eslint . --config=.eslintrc.js --ignore-path .eslintignore",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch"
  },
  "author": "VL Team",
  "license": "ISC",
