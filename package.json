    "supertest": "^6.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/src/.*)\\.(test|spec).js$",
    "modulePathIgnorePatterns": [
      "(/src/middleware/.*)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/middlewares/**",
      "!src/utils/**",
      "!src/services/aws/**",
      "!src/services/tex/**"
    ]
  }
