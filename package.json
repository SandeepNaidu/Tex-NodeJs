    "@app": "src",
    "@api": "src/api",
    "@boot": "src/boot",
    "@config": "src/config",
    "@middlewares": "src/middlewares",
    "@models": "src/models",
    "@services": "src/services",
    "@utils": "src/utils"
  },
  "devDependencies": {
    "electron-builder": "^23.6.0",
    "electron-packager": "^16.0.0",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
